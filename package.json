  "homepage": "https://daysnap.github.io/cli/",
  "engines": {
    "node": ">=14.16.1",
    "npm": ">=6.14.12"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "commitizen": "^4.2.4",
